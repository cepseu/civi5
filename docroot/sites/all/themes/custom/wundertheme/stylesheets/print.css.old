/**
 * full width titles on panes
 */
.wide-title .pane-title {
  font-family: "Flama Basic", Helvetica, Arial, sans-serif;
  font-size: 30px;
  margin: 100px 0;
  text-align: center;
  max-width: none;
  padding: 0;
  position: relative;
  text-transform: uppercase;
  width: auto;
}
.wide-title .pane-title:before {
  border-bottom: 2px solid #d3d2d2;
  bottom: 14px;
  content: " ";
  display: block;
  position: absolute;
  width: 100%;
}
.wide-title .pane-title > span {
  background: white;
  display: inline-block;
  line-height: 30px;
  padding: 0 1em;
  position: relative;
}
.panel-col-bottom .wide-title .pane-title, .wide-title .panel-col-bottom .pane-title {
  margin: 50px 0;
}
.panel-col-bottom .wide-title .pane-title:before, .wide-title .panel-col-bottom .pane-title:before {
  border-bottom: none;
  content: "";
}
.panel-col-bottom .wide-title .pane-title > span, .wide-title .panel-col-bottom .pane-title > span {
  background: transparent;
}

/**
 * page-title
 */
h1.page-title {
  text-align: center;
}
h1.page-title .page-title-inside {
  /*border: 4px solid $base-text-color;*/
  display: inline-block;
