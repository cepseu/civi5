/**
 * messages
 */
.messages {
  background: #c2f75f;
  padding: 20px;
  margin: 1em 0;
  min-height: 56px;
}
.messages:before {
  background: url("../images/messages.svg") 0 0 no-repeat;
  content: " ";
  display: block;
  float: left;
  height: 32px;
  margin: -7px 8px 0 0;
  width: 32px;
}
.messages .message {
  margin-left: 40px;
}
.messages .message em {
  font-weight: bold;
}
.messages .message ~ .message {
  border-top: 2px dotted rgba(255, 255, 255, 0.6);
  margin-top: 0.5em;
  padding-top: 0.5em;
}
.messages.error {
  background-color: #f7765f;
}
.messages.error .message {
  border-color: rgba(255, 255, 255, 0.3);
}
.messages.error:before {
  background-position: 0 -64px;
}
.messages.warning {
  background-color: #fff05d;
}
.messages.warning .message {
  border-color: rgba(255, 255, 255, 0.8);
}
.messages.warning:before {
  background-position: 0 -32px;
}
.messages .krumo-nest {
  padding-left: 1em;
}

