/**
 * admin tabs
 */
.pane-page-tabs,
nav.tabs {
  margin: 12px 0;
}
.pane-page-tabs .tabs,
nav.tabs .tabs {
  list-style: none;
  padding: 0;
  margin: 0;
  font-size: 0;
  margin: 0;
}
.pane-page-tabs .tabs li,
nav.tabs .tabs li {
  list-style: none;
  margin: 0;
}
.pane-page-tabs .tabs li,
nav.tabs .tabs li {
  display: inline-block;
  font-size: 16px;
  list-style: none;
  margin: 0;
  padding: 0;
}
.pane-page-tabs .tabs > li > a,
.pane-page-tabs .tabs > li .nolink,
nav.tabs .tabs > li > a,
nav.tabs .tabs > li .nolink {
  display: inline-block;
  text-align: center;
  text-decoration: none;
}
.pane-page-tabs .tabs > li.expanded,
nav.tabs .tabs > li.expanded {
  position: relative;
}
.pane-page-tabs .tabs > li:hover ul.menu,
nav.tabs .tabs > li:hover ul.menu {
  left: 0;
  z-index: 99;
}
.pane-page-tabs .tabs > li:hover a,
nav.tabs .tabs > li:hover a {
  white-space: nowrap;
}
.pane-page-tabs .tabs .lvl-3,
nav.tabs .tabs .lvl-3 {
  position: absolute;
  left: -9999em;
  margin: 0;
}
.pane-page-tabs .tabs .lvl-3 li,
nav.tabs .tabs .lvl-3 li {
  display: block;
}
.pane-page-tabs .tabs a,
nav.tabs .tabs a {
  height: 30px;
  line-height: 30px;
  margin-right: 1em;
  padding: 0 12px;
}
.pane-page-tabs .primary a,
nav.tabs .primary a {
  border: 1px solid #b1b1b1;
  border-radius: 2px;
  -webkit-transition: background-color 0.3s ease;
  -moz-transition: background-color 0.3s ease;
  -o-transition: background-color 0.3s ease;
  transition: background-color 0.3s ease;
}
.pane-page-tabs .primary a.active,
nav.tabs .primary a.active {
  background: #f7f7f7;
}
.pane-page-tabs .primary a:hover,
nav.tabs .primary a:hover {
  background: #f7f7f7;
}
.pane-page-tabs .secondary,
nav.tabs .secondary {
  background: #d3d2d2;
}
.pane-page-tabs .secondary a:hover,
nav.tabs .secondary a:hover {
  text-decoration: underline;
}

