}
.page-search .region-content .search-form input[type="text"] {
  margin-bottom: 0.5em;
}
.page-search .region-content .view-mode-search_result {
  margin: 20px 0;
}

ol.search-results {
  padding: 0;
}
ol.search-results li {
  margin-bottom: 1.25em;
}
ol.search-results li h3 {
  margin-bottom: 0.75em;
}
